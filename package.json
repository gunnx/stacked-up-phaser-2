{
  "name": "stacked-up",
  "version": "0.1.0",
  "description": "Tetris clone",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development webpack --config webpack.config.js",
    "devWatch": "npm run dev -- --watch",
    "serve": "browser-sync start --port 3000 --ui-port 3001 --server './public' --files './public/css/*.css' './public/js/*.js' './public/*.html'",
    "prod": "NODE_ENV=production webpack webpack --env.prod=true --config webpack.config.js",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gunnx/stacked-up.git"
  },
  "keywords": [
    "tetris",
    "game",
    "phaser"
  ],
  "author": "Keith Gunn <keith.gunn@tescobank.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gunnx/stacked-up/issues"
  },
  "homepage": "https://github.com/gunnx/stacked-up#readme",
  "dependencies": {
    "phaser-ce": "2.8.2"
  },
  "devDependencies": {
    "babel-core": "6.25.0",
    "babel-loader": "7.1.1",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "browser-sync": "2.18.13",
    "browser-sync-webpack-plugin": "1.2.0",
    "copy-webpack-plugin": "4.0.1",
    "expose-loader": "0.7.3",
    "html-webpack-plugin": "2.29.0",
    "webfontloader": "1.6.28",
    "webpack": "3.3.0"
  }
}
